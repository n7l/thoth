{
    "manifest_version": 3,
    "name": "THOTH",
    "version": "1.0",
    "description": "Collects information about open tabs.",
    "permissions": ["tabs"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "pin-icon.png",
        "48": "pin-icon.png",
        "128": "pin-icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }